<Application x:Class="TelerikWpfDemo1.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerikWpfDemo1="clr-namespace:TelerikWpfDemo1"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
      <ResourceDictionary>
          <ResourceDictionary.MergedDictionaries>
            <ResourceDictionary Source="Themes/ExpressionDark/System.Windows.xaml" />
            <ResourceDictionary Source="Themes/ExpressionDark/Telerik.Windows.Controls.xaml" />
            <ResourceDictionary Source="Themes/ExpressionDark/Telerik.Windows.Controls.Input.xaml" />
          </ResourceDictionary.MergedDictionaries>

            <telerikWpfDemo1:DataViewModel x:Key="DataModel" />

        </ResourceDictionary>
    </Application.Resources>
</Application>